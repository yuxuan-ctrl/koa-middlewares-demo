{
  "name": "koa-middlewares",
  "version": "1.0.0",
  "description": "koa2项目与中间件练手",
  "main": "index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node --files src/index.ts",
    "dev:debug": "cross-env NODE_ENV=development nodemon --debug --exec ts-node --files src/index.ts",
    "build": "rimraf dist && tsc",
    "serve:uat": "pm2 startOrReload ecosystem.config.js --env uat",
    "serve:prod": "pm2 startOrReload ecosystem.config.js --env prod"
  },
  "keywords": [
    "koa2",
    "redis",
    "session",
    "kafka"
  ],
  "author": "yuxuan-ctrl",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/multer": "^3.0.2",
    "@koa/router": "^12.0.1",
    "amqplib": "^0.10.4",
    "assert": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cls-hooked": "^4.2.2",
    "crypto": "^1.0.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-redis": "^4.0.1",
    "koa-session": "^6.4.0",
    "koa2-swagger-ui": "^5.10.0",
    "log4js": "^6.9.1",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.4",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-redis": "^4.0.5",
    "@types/koa-session": "^6.4.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "cross-env": "^7.0.3",
    "koa-swagger-decorator": "^1.8.7",
    "nodemon": "^3.1.0",
    "pm2": "^5.3.1",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
